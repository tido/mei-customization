    <xs:attributeGroup ref="mei:tido_att.stems.attributes"/>
    <xs:attributeGroup ref="mei:tido_att.typography.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tido_att.octave.attributes">
    <xs:attributeGroup ref="mei:tido_att.octave.attribute.oct"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tido_att.octave.attribute.oct">
    <xs:attribute name="oct" type="mei:tido_data.OCTAVE">
      <xs:annotation>
        <xs:documentation>Captures written octave information.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="tido_att.ornam.log.attributes">
    <xs:attributeGroup ref="mei:tido_att.controlevent.attributes"/>
    <xs:attributeGroup ref="mei:tido_att.startendid.attributes"/>
    <xs:attributeGroup ref="mei:tido_att.timestamp2.musical.attributes"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="tido_att.ornam.vis.attributes">
