               <ref name="tido_model.appLike"/>
               <ref name="tido_model.editLike"/>
               <ref name="tido_model.transcriptionLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido.sibelius-note-require_out_of_chord-constraint-35">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:tido="http://tido-music.com/ns/1.0"
                      context="mei:note">
                  <sch:assert test="if (not(ancestor::mei:chord) and not(@dur)) then false() else true()">
                    Must have dur attribute.</sch:assert>
                </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido.sibelius-note-no_stem_in_chord-constraint-36">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:tido="http://tido-music.com/ns/1.0"
                      context="mei:note[ancestor::mei:chord]">
                  <sch:report test="@stem.dir or @stem.len or @stem.pos or @stem.x or @stem.y"> Stem
                    attributes not permitted on notes within a chord.</sch:report>
                </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
