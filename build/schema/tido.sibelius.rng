         <ref name="tido_att.authorized.attributes"/>
         <ref name="tido_att.color.attributes"/>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.extsym.attributes"/>
         <ref name="tido_att.scalable.attributes"/>
         <ref name="tido_att.startid.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <ref name="tido_att.typography.attributes"/>
         <ref name="tido_att.visualoffset.attributes"/>
         <ref name="tido_att.xy.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_att.duration.musical.req.attributes">
      <ref name="tido_att.duration.musical.req.attribute.dur"/>
   </define>
   <define name="tido_att.duration.musical.req.attribute.dur">
      <attribute name="dur">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">records the duration of a feature using the relative durational values provided by the data.DURATION datatype.</a:documentation>
         <ref name="tido_data.DURATION"/>
      </attribute>
   </define>
   <define name="tido_att.timestamp.musical.req.attributes">
      <ref name="tido_att.timestamp.musical.req.attribute.tstamp"/>
   </define>
   <define name="tido_att.timestamp.musical.req.attribute.tstamp">
      <attribute name="tstamp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">encodes the onset time in terms of musical time, i.e., beats[.fractional_beat_part].</a:documentation>
         <ref name="tido_data.BEAT"/>
      </attribute>
   </define>
   <define name="tido_att.timestamp2.musical.req.attributes">
      <ref name="tido_att.timestamp2.musical.req.attribute.tstamp2"/>
   </define>
   <define name="tido_att.timestamp2.musical.req.attribute.tstamp2">
      <attribute name="tstamp2">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">encodes the ending point of an event in terms of musical time, i.e., a count of measures plus a beat location.</a:documentation>
         <ref name="tido_data.MEASUREBEAT.strict"/>
      </attribute>
   </define>
   <define name="tido_att.ornam.altsym.attributes">
      <ref name="tido_att.ornam.altsym.attribute.altsym"/>
   </define>
   <define name="tido_att.ornam.altsym.attribute.altsym">
      <optional>
         <attribute name="altsym">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides a way of pointing to an ornament symbol. It must contain an ID of a &lt;symbolDef&gt; element in Tido's ornament symbol table.</a:documentation>
            <choice>
               <value>#ornamentMordent</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Inverted mordent (Tido default symbol for mordent[@inv='true']).</a:documentation>
               <value>#ornamentMordentInverted</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Mordent (Tido default symbol for mordent[not(@inv='true')]).</a:documentation>
               <value>#ornamentTurn</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Turn (Tido default symbol for turn[not(@inv='true')]).</a:documentation>
               <value>#ornamentTurnSlash</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Turn with slash (Tido default symbol for turn[@inv='true']).</a:documentation>
               <value>#ornamentTrill</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Trill.</a:documentation>
               <value>#ornamentPrecompSlideTrillDAnglebert</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Slide trill (D'Anglebert).</a:documentation>
               <value>#ornamentPrecompMordentUpperPrefix</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Mordent with upper prefix.</a:documentation>
               <value>#ornamentPrecompTrillSuffixDandrieu</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Trill with suffix (Dandrieu).</a:documentation>
               <value>#ornamentPrecompTrillLowerSuffix</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Trill with lower suffix.</a:documentation>
               <value>#ornamentPrecompPortDeVoixMordent</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Port de voix mordent.</a:documentation>
               <value>#ornamentPrecompInvertedMordentUpperPrefix</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Inverted mordent with upper prefix.</a:documentation>
               <value>#ornamentPrecompAppoggTrill</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Appogiatura trill.</a:documentation>
               <value>#ornamentTremblement</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tremblement (Tido default symbol for mordent[@long='true']).</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tido_att.controlevent.strict.attributes">
      <ref name="tido_att.plist.attributes"/>
      <ref name="tido_att.timestamp.musical.req.attributes"/>
      <ref name="tido_att.timestamp.performed.attributes"/>
      <ref name="tido_att.staffident.req.attributes"/>
      <ref name="tido_att.layerident.attributes"/>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="tido.sibelius-att.controlevent.strict-require_layer_when_staff_is_not_multivalued-constraint-90">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xi="http://www.w3.org/2001/XInclude"
                xmlns:tido="http://tido-music.com/ns/1.0"
                context="mei:slur|mei:hairpin">
                  <sch:assert test="if (not(contains(@staff, ' ')) and not(@layer)) then false() else true()">@layer must be specified unless @staff is multi-valued.</sch:assert>
                </sch:rule>
   </pattern>
   <define name="tido_att.staffident.req.attributes">
      <ref name="tido_att.staffident.req.attribute.staff"/>
   </define>
   <define name="tido_att.staffident.req.attribute.staff">
      <attribute name="staff">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">signifies the staff on which a notated event occurs or to which a control event applies. Mandatory when applicable.</a:documentation>
         <list>
            <oneOrMore>
               <data type="positiveInteger"/>
            </oneOrMore>
         </list>
      </attribute>
   </define>
   <define name="tido_att.slur.log.strict.attributes">
      <ref name="tido_att.controlevent.strict.attributes"/>
      <ref name="tido_att.augmentdots.attributes"/>
      <ref name="tido_att.duration.additive.attributes"/>
      <ref name="tido_att.startendid.attributes"/>
      <ref name="tido_att.timestamp2.musical.req.attributes"/>
   </define>
   <define name="tido_att.hairpin.log.strict.attributes">
      <ref name="tido_att.controlevent.strict.attributes"/>
      <ref name="tido_att.augmentdots.attributes"/>
      <ref name="tido_att.duration.additive.attributes"/>
      <ref name="tido_att.startendid.attributes"/>
      <ref name="tido_att.timestamp2.musical.req.attributes"/>
      <ref name="tido_att.hairpin.log.strict.attribute.form"/>
   </define>
   <define name="tido_att.hairpin.log.strict.attribute.form">
      <attribute name="form">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">captures the visual rendition and function of the hairpin; that is, whether it indicates an increase or a decrease in volume.</a:documentation>
         <choice>
            <value>cres</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">crescendo; i.e., louder.</a:documentation>
            <value>dim</value>
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">diminuendo; i.e., softer.</a:documentation>
         </choice>
      </attribute>
   </define>
   <define name="tido_style">
      <element name="style" ns="http://tido-music.com/ns/1.0">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a reference to a Tido Stylesheet (TSS).</a:documentation>
         <text/>
         <ref name="tido_att.id.attributes"/>
         <optional>
            <attribute name="src">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies the location of the associated Tido Stylesheet (TSS).</a:documentation>
               <ref name="tido_data.URI"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tido_att.tido.color.attributes">
      <ref name="tido_att.tido.color.attribute.color"/>
   </define>
   <define name="tido_att.tido.color.attribute.color">
      <optional>
         <attribute name="color" ns="http://tido-music.com/ns/1.0">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">records color information as required by content creation workflow for tido. Further processing should transform this information into Critical Apparatus, Editorial Markup or other standard MEI. See discussion: https://github.com/tido/MEIConverter/issues/5 </a:documentation>
            <ref name="tido_data.COLOR"/>
         </attribute>
      </optional>
   </define>
