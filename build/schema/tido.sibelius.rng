   <define name="tido_chord">
      <element name="chord">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A simultaneous sounding of two or more notes in the same layer *with the same duration*.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="tido_note"/>
               <ref name="tido_artic"/>
               <ref name="tido_model.editLike"/>
               <ref name="tido_model.transcriptionLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido.sibelius-chord-chord_notes_same_dur-constraint-24">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:tido="http://tido-music.com/ns/1.0"
                      context="mei:chord[@dur][count(distinct-values(for $n in descendant::mei:note return $n/@dur)) &gt; 0]">
                  <sch:let name="durs"
                        value="distinct-values(for $n in descendant::mei:note return $n/@dur)"/>
                  <sch:let name="durs_count"
                        value="count(distinct-values($durs)) + count(descendant::mei:note[not(@dur)])"/>
                  <sch:report test="$durs_count &gt; 1 or not($durs[1] = @dur)"> Duration of chord
                    and constituent notes must match.</sch:report>
                </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.chord.log.attributes"/>
         <ref name="tido_att.chord.vis.attributes"/>
         <ref name="tido_att.chord.ges.attributes"/>
         <ref name="tido_att.chord.anl.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_clef">
      <element name="clef">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indication of the exact location of a particular note on the staff and, therefore, the other notes as well.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido.sibelius-clef-Clef_position_lines-constraint-25">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:tido="http://tido-music.com/ns/1.0"
                      context="mei:clef[ancestor::mei:staffDef[@lines]]">
               <sch:let name="thisstaff" value="ancestor::mei:staffDef/@n"/>
               <sch:assert test="number(@line) &lt;=               number(ancestor::mei:staffDef[@n=$thisstaff and @lines][1]/@lines)">The clef position must be less than or equal to the number of lines on the
              staff.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido.sibelius-clef-Clef_position_nolines-constraint-26">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:tido="http://tido-music.com/ns/1.0"
                      context="mei:clef[ancestor::mei:staffDef[not(@lines)]]">
               <sch:let name="thisstaff" value="ancestor::mei:staffDef/@n"/>
               <sch:assert test="number(@line) &lt;=               number(preceding::mei:staffDef[@n=$thisstaff and @lines][1]/@lines)">The clef position must be less than or equal to the number of lines on the
              staff.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.event.attributes"/>
         <ref name="tido_att.clef.anl.attributes"/>
         <ref name="tido_att.clef.ges.attributes"/>
         <ref name="tido_att.clef.log.attributes"/>
         <ref name="tido_att.clef.vis.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_composer">
