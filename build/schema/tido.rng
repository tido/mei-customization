         <ref name="tido_model.controleventLike"/>
         <ref name="tido_model.staffLike"/>
         <ref name="tido_model.ossiaLike"/>
         <ref name="tido_model.lyricsLike"/>
      </choice>
   </define>
   <define name="tido_model.ossiaLike">
      <notAllowed/>
   </define>
   <define name="tido_model.sectionPart.cmn">
      <choice>
         <ref name="tido_model.measureLike"/>
      </choice>
   </define>
   <define name="tido_beam">
      <element name="beam">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A container for a series of explicitly beamed events that begins and ends entirely within a measure.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="tido_model.eventLike"/>
               <ref name="tido_model.appLike"/>
               <ref name="tido_model.editLike"/>
               <ref name="tido_model.transcriptionLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-beam-When_not_copyof_beam_content-constraint-40">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:beam[not(@copyof)]">
               <sch:assert test="count(descendant::*[local-name()='note' or local-name()='rest' or               local-name()='chord' or local-name()='space']) &gt; 1">A beam without a copyof attribute must have at least 2 note, rest, chord, or space
              descendants.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_fermata">
      <element name="fermata">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An indication placed over a note or rest to indicate that it should be held longer than its written value. May also occur over a bar line to indicate the end of a phrase or section. Sometimes called a 'hold' or 'pause'.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-fermata-fermata_start-type_attributes_required-constraint-41">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:fermata">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.fermata.log.attributes"/>
         <ref name="tido_att.fermata.vis.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_hairpin">
      <element name="hairpin">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates continuous dynamics expressed on the score as wedge-shaped graphics, e.g. &lt; and &gt;.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-hairpin-hairpin_place_required_with_single_staff-constraint-42">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:hairpin[not(contains(@staff, ' '))]">
                  <sch:assert test="exists(@place)">Must have place attribute.</sch:assert>
                </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.hairpin.log.attributes"/>
         <ref name="tido_att.hairpin.vis.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_measure">
      <element name="measure">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Unit of musical time consisting of a fixed number of note-values of a given type, as determined by the prevailing meter, and delimited in musical notation by bar lines.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="tido_model.appLike"/>
               <ref name="tido_model.divLike"/>
               <ref name="tido_model.milestoneLike.music"/>
               <ref name="tido_model.staffDefLike"/>
               <ref name="tido_model.annotLike"/>
               <ref name="tido_model.graphicprimitiveLike"/>
               <ref name="tido_model.editLike"/>
               <ref name="tido_model.transcriptionLike"/>
               <ref name="tido_model.measurePart"/>
            </choice>
         </zeroOrMore>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.measure.log.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_meterSig">
      <element name="meterSig">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(meter signature) – Written meter signature.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-meterSig-meter_att_pair-constraint-43">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:meterSig">
                  <sch:report test="not(@count and @unit)">Time signature must be complete (both
                    @count and @unit are required).</sch:report>
                </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.meterSig.log.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_mRest">
      <element name="mRest">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(measure rest) – Complete measure rest in any meter. </a:documentation>
         <empty/>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.mRest.log.attributes"/>
         <ref name="tido_att.mRest.vis.attributes"/>
         <ref name="tido_att.mRest.ges.attributes"/>
         <ref name="tido_att.mRest.anl.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_pedal">
      <element name="pedal">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Piano pedal mark.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-pedal-pedal_style_consistent-constraint-44">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:pedal[@form][@dir=('bounce', 'up')]">
                  <sch:let name="form" value="@form"/>
                  <sch:assert test="preceding::mei:pedal[1][@form=$form]">
                    Pedal styles must be consistent between pedal down and pedal up.</sch:assert>
                </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-pedal-pedal_start-type_attributes_required-constraint-45">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:pedal">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.pedal.log.attributes"/>
         <ref name="tido_att.pedal.vis.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_slur">
      <element name="slur">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indication of 1) a "unified melodic idea" or 2) performance technique.</a:documentation>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-slur-slur_start-_and_end-type_attributes_required-constraint-46">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:slur">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
               <sch:assert test="@dur or @dur.ges or @endid or @tstamp2">Must have one of the
              attributes: dur, dur.ges, endid, or tstamp2</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.slur.log.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_tie">
      <element name="tie">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An indication that two notes of the same pitch form a single note with their combined rhythmic values.</a:documentation>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-tie-tie_start-_and_end-type_attributes_required-constraint-47">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:tie">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
               <sch:assert test="@dur or @dur.ges or @endid or @tstamp2">Must have one of the
              attributes: dur, dur.ges, endid, or tstamp2</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-tie-tie_containing_curve-constraint-48">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:tie[mei:curve[@bezier or @bulge or @curvedir or @lform or @lwidth or             @ho or @startho or @endho or @to or @startto or @endto or @vo or @startvo or             @endvo or @x or @y or @x2 or @y2]]">
               <sch:assert test="not(@bezier or @bulge or @curvedir or @lform or @lwidth or @ho or @startho or               @endho or @to or @startto or @endto or @vo or @startvo or @endvo or @x or @y or @x2 or @y2)"
                           role="warning">The visual attributes of the tie (@bezier, @bulge, @curvedir, @lform,
              @lwidth, @ho, @startho, @endho, @to, @startto, @endto, @vo, @startvo, @endvo, @x, @y,
              @x2, and @y2) will be overridden by visual attributes of the contained curve
              elements.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.tie.log.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_att.mordent.log.attributes">
      <ref name="tido_att.controlevent.attributes"/>
      <ref name="tido_att.startendid.attributes"/>
      <ref name="tido_att.mordent.log.attribute.form"/>
      <ref name="tido_att.mordent.log.attribute.long"/>
   </define>
   <define name="tido_att.mordent.log.attribute.form">
      <optional>
         <attribute name="form">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Traditionally, the 'normal' mordent is written as a short wavy line with a vertical line through it and the inverted mordent is written without the vertical line. However, the meaning of these signs is sometimes reversed. See Read, p. 245-246. Another attribute in the visual domain would be necessary in order to be completely explicit about which visual symbol is actually to be rendered.</a:documentation>
            <choice>
               <value>inv</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Inverted mordent, e.g., performed as the principal note, followed by its upper neighbor, with a return to the principal note.</a:documentation>
               <value>norm</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"normal" mordent, e.g., performed as the written note, followed by its lower neighbor, with a return to the written note.</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tido_att.mordent.log.attribute.long">
      <optional>
         <attribute name="long">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">When the long attribute is set to 'yes', a double or long mordent, consisting of 5 notes, is indicated.</a:documentation>
            <ref name="tido_data.BOOLEAN"/>
         </attribute>
      </optional>
   </define>
   <define name="tido_att.mordent.vis.attributes">
      <ref name="tido_att.ornam.extsym.attributes"/>
      <ref name="tido_att.placement.attributes"/>
      <ref name="tido_att.typography.attributes"/>
   </define>
   <define name="tido_att.trill.log.attributes">
      <ref name="tido_att.controlevent.attributes"/>
      <ref name="tido_att.augmentdots.attributes"/>
      <ref name="tido_att.startendid.attributes"/>
      <ref name="tido_att.timestamp2.musical.attributes"/>
   </define>
   <define name="tido_att.trill.vis.attributes">
      <ref name="tido_att.ornam.extsym.attributes"/>
      <ref name="tido_att.placement.attributes"/>
      <ref name="tido_att.typography.attributes"/>
   </define>
   <define name="tido_att.turn.log.attributes">
      <ref name="tido_att.controlevent.attributes"/>
      <ref name="tido_att.startid.attributes"/>
      <ref name="tido_att.turn.log.attribute.delayed"/>
      <ref name="tido_att.turn.log.attribute.form"/>
   </define>
   <define name="tido_att.turn.log.attribute.delayed">
      <optional>
         <attribute name="delayed">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">When the delayed attribute is set to 'true', the turn begins on the second half of the beat. See Read, p. 246.</a:documentation>
            <ref name="tido_data.BOOLEAN"/>
         </attribute>
      </optional>
   </define>
   <define name="tido_att.turn.log.attribute.form">
      <optional>
         <attribute name="form">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates the style of the turn.</a:documentation>
            <choice>
               <value>inv</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Inverted turn, e.g., begins on the note below the written note.</a:documentation>
               <value>norm</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"normal" turn, e.g., begins on the note above the written note.</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tido_att.turn.vis.attributes">
      <ref name="tido_att.ornam.extsym.attributes"/>
      <ref name="tido_att.placement.attributes"/>
      <ref name="tido_att.typography.attributes"/>
   </define>
   <define name="tido_model.ornamentLike.cmn">
      <choice>
         <ref name="tido_mordent"/>
         <ref name="tido_trill"/>
         <ref name="tido_turn"/>
      </choice>
   </define>
   <define name="tido_model.ornamentLike.cmn_alternation">
      <choice>
         <ref name="tido_mordent"/>
         <ref name="tido_trill"/>
         <ref name="tido_turn"/>
      </choice>
   </define>
   <define name="tido_model.ornamentLike.cmn_sequence">
      <ref name="tido_mordent"/>
      <ref name="tido_trill"/>
      <ref name="tido_turn"/>
   </define>
   <define name="tido_model.ornamentLike.cmn_sequenceOptional">
      <optional>
         <ref name="tido_mordent"/>
      </optional>
      <optional>
         <ref name="tido_trill"/>
      </optional>
      <optional>
         <ref name="tido_turn"/>
      </optional>
   </define>
   <define name="tido_model.ornamentLike.cmn_sequenceOptionalRepeatable">
      <zeroOrMore>
         <ref name="tido_mordent"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="tido_trill"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="tido_turn"/>
      </zeroOrMore>
   </define>
   <define name="tido_model.ornamentLike.cmn_sequenceRepeatable">
      <oneOrMore>
         <ref name="tido_mordent"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_trill"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_turn"/>
      </oneOrMore>
   </define>
   <define name="tido_mordent">
      <element name="mordent">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An ornament indicating rapid alternation of the main note with a secondary note, usually a step below, but sometimes a step above. </a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-mordent-mordent_start-type_attributes_required-constraint-49">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:mordent">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.mordent.log.attributes"/>
         <ref name="tido_att.mordent.vis.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_trill">
      <element name="trill">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Rapid alternation of a note with another (usually at the interval of a second above).</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-trill-trill_start-type_attributes_required-constraint-50">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:trill">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.trill.log.attributes"/>
         <ref name="tido_att.trill.vis.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_turn">
      <element name="turn">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An ornament consisting of four notes — the upper neighbor of the written note, the written note, the lower neighbor, and the written note.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="tido-turn-turn_start-type_attributes_required-constraint-51">
            <sch:rule xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="mei:turn">
               <sch:assert test="@startid or @tstamp or @tstamp.ges or @tstamp.real">Must have one of
              the attributes: startid, tstamp, tstamp.ges or tstamp.real</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.turn.log.attributes"/>
         <ref name="tido_att.turn.vis.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_model.appLike">
      <notAllowed/>
   </define>
   <define name="tido_model.choicePart">
      <choice>
         <ref name="tido_model.editorialLike"/>
         <ref name="tido_model.editLike"/>
         <ref name="tido_orig"/>
         <ref name="tido_reg"/>
      </choice>
   </define>
   <define name="tido_model.editLike">
      <choice>
         <ref name="tido_choice"/>
      </choice>
   </define>
   <define name="tido_model.transcriptionLike">
      <choice>
         <ref name="tido_orig"/>
         <ref name="tido_reg"/>
      </choice>
   </define>
