      <oneOrMore>
         <ref name="tido_model.biblLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_model.dateLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_model.editorialLike_sequenceRepeatable"/>
