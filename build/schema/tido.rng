      <element name="choice">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Groups a number of alternative encodings for the same point in a text.</a:documentation>
         <zeroOrMore>
            <ref name="tido_model.choicePart"/>
         </zeroOrMore>
         <ref name="tido_att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_orig">
      <element name="orig">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(original) – Contains material which is marked as following the original, rather than being normalized or corrected.</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="tido_model.textphraseLike"/>
               <ref name="tido_model.eventLike"/>
               <ref name="tido_model.controleventLike"/>
               <ref name="tido_model.lyricsLike"/>
               <ref name="tido_model.editLike"/>
               <ref name="tido_model.transcriptionLike"/>
               <ref name="tido_model.eventLike.measureFilling"/>
               <ref name="tido_model.noteModifierLike"/>
               <ref name="tido_model.sectionLike"/>
               <ref name="tido_model.measureLike"/>
               <ref name="tido_model.staffLike"/>
               <ref name="tido_model.layerLike"/>
               <ref name="tido_model.graphicprimitiveLike"/>
            </choice>
         </zeroOrMore>
         <ref name="tido_att.common.attributes"/>
         <ref name="tido_att.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tido_reg">
      <element name="reg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(regularization) – Contains material which has been regularized or normalized in some sense.</a:documentation>
         <zeroOrMore>
