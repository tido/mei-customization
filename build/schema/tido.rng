      <oneOrMore>
         <ref name="tido_model.identifierLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_model.lbLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_model.measurementLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="tido_model.nameLike_sequenceRepeatable"/>
